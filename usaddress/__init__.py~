import pycrfsuite
#predictions

TAGGER = pycrfsuite.Tagger()
TAGGER.open('usaddr.crfsuite')

def parse(address_string) :
    return TAGGER.tag(address_string.split())

print parse('123 Main St, Chicago IL, 60647')


